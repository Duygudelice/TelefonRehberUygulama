
@model telefonrehberuygulaması.Models.sınıflar.Rehber
@{
    Layout = null;
}
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css"> </link>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">

<h2>Yeni Kayıt Ekleme</h2>
<br />
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.AdSoyad)
    @Html.TextBoxFor(x => x.AdSoyad, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.numara)
    @Html.TextBoxFor(x => x.numara, new { @class = "form-control" })

    <br />

    <button class="btn btn-primary">Bloğu Ekle</button>
</form>
